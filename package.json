{
	"name": "commit-test",
	"displayName": "commit-test",
	"description": "Jenkins-based commit-test",
	"version": "0.0.3",
	"publisher": "zigman",
	"homepage": "http://172.26.6.129/jy.hsu/vscode-commit-test/blob/master/README.md",
	"icon": "images/commit-test.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/zigman-dev/vscode-commit-test.git"
	},
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:commit-test.verifyEnvironment",
		"onCommand:commit-test.commitTest",
		"onCommand:commit-test.commitTestChangelist",
		"onCommand:commit-test.getTicketChangelist"
	],
	"main": "./out/extension.js",
	"contributes": {
		"_FIXME": "Add descriptions",
		"commands": [
			{
				"command": "commit-test.verifyEnvironment",
				"title": "Verify Environment",
				"category": "Commit-Test"
			},
			{
				"command": "commit-test.commitTest",
				"title": "Submit Commit-Test",
				"category": "Commit-Test"
			},
			{
				"command": "commit-test.commitTestChangelist",
				"title": "Submit Commit-Test for Changelist",
				"category": "Commit-Test",
				"icon": "$(run)"
			},
			{
				"command": "commit-test.getTicketChangelist",
				"title": "Get Ticket for Changlist",
				"category": "Commit-Test",
				"icon": "$(bookmark)"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "commit-test.commitTestChangelist",
					"when": "false"
				},
				{
					"command": "commit-test.getTicketChangelist",
					"when": "false"
				}
			],
			"scm/resourceGroup/context": [
				{
					"command": "commit-test.commitTestChangelist",
					"when": "config.svn.enabled && scmProvider == svn && scmResourceGroup != unversioned",
					"group": "inline"
				},
				{
					"command": "commit-test.getTicketChangelist",
					"when": "config.svn.enabled && scmProvider == svn && scmResourceGroup in commit-test:tickets",
					"group": "inline"
				}
			]
		},
		"configuration": {
			"title": "Commit-Test",
			"properties": {
				"commit-test.jenkins.account.user": {
					"type": "string",
					"scope": "resource"
				},
				"commit-test.jenkins.account.password": {
					"type": "string",
					"scope": "resource"
				},
				"commit-test.jenkins.account.mail": {
					"type": "string",
					"scope": "resource"
				},
				"commit-test.jenkins.hostAddress": {
					"type": "string",
					"format": "uri",
					"scope": "resource",
					"default": "http://172.26.6.130:8080"
				},
				"commit-test.jenkins.jobName": {
					"type": "string",
					"scope": "resource",
					"default": "mainline/commit_test"
				}
			},
			"default": {
				"commit-test.jenkins.hostAddress": "http://172.26.6.130:8080",
				"commit-test.jenkins.jobName": "mainline/commit_test"
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"form-data": "^3.0.0",
		"jenkins": "^0.28.1",
		"svn-interface": "^0.5.0"
	}
}
